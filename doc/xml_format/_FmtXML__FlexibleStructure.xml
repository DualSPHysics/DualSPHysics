	<special>
		<structures>
			<structure mkbound="1">
				<Density value="1161.54" comment="Structure material density" units_comment="kg/m^3" />
				<YoungMod value="3.5e6" comment="Young's Modulus" units_comment="Pa" />
				<PoissonRatio value="0.49" comment="Poisson ratio" />
				<ConstModel value="StVenantKirchhoff" comment="Constitutive model (PlaneStrain, PlaneStress, StVenantKirchhoff)" />
