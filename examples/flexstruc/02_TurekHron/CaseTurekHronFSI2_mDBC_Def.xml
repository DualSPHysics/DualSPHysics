<?xml version="1.0" encoding="UTF-8" ?>
<case>
    <casedef>
        <constantsdef>
            <gravity x="0" y="0" z="0" comment="Gravitational acceleration" units_comment="m/s^2" />
            <rhop0 value="1000" comment="Reference density of the fluid" units_comment="kg/m^3" />
            <rhopgradient value="1" comment="Initial density gradient 1:Rhop0, 2:Water column, 3:Max. water height (default=2)" />
            <hswl value="0.0" auto="false" comment="Maximum still water level to calculate speedofsound using coefsound" units_comment="metres (m)" />
            <gamma value="7" comment="Polytropic constant for water used in the state equation" />
            <speedsystem value="5" auto="false" comment="Maximum system speed (by default the dam-break propagation is used)" />
            <coefsound value="10" comment="Coefficient to multiply speedsystem" />
            <speedsound value="0" auto="true" comment="Speed of sound to use in the simulation (by default speedofsound=coefsound*speedsystem)" />
            <coefh value="1.0" comment="Coefficient to calculate the smoothing length (h=coefh*sqrt(3*dp^2) in 3D)" />		
            <cflnumber value="0.1" comment="Coefficient to multiply dt" />	
        </constantsdef>	
        <mkconfig boundcount="240" fluidcount="10" />
        <geometry>
            <predefinition>
                <newvarcte cyl_freedraw="true" mdbc="true" />
                <newvarcte dom_padding="0.1" inout_layers="8" />
                <newvarcte fluid_min_x="0.0" fluid_max_x="2.5" fluid_min_z="0.0" fluid_max_z="0.41" />
                <newvarcte cyl_cen_x="0.2" cyl_cen_z="0.2" cyl_radius="0.05" />
                <newvarcte struc_min_x="#cyl_cen_x" struc_max_x="0.6" struc_min_z="0.19" struc_max_z="0.21" />
            </predefinition>
            <definition dp="0.0025" units_comment="metres (m)">
                <pointref x="0" y="0" z="0" />
                <pointmin x="#fluid_min_x-(inout_layers+4)*Dp-dom_padding" y="0.0" z="#fluid_min_z-dom_padding" />
                <pointmax x="#fluid_max_x+(inout_layers+4)*Dp+dom_padding" y="0.0" z="#fluid_max_z+dom_padding" />
            </definition>
            <commands>
                <list name="GeometryForNormals">
                    <setactive drawpoints="0" drawshapes="1"/>
                    <setshapemode>actual | bound</setshapemode>
                    <setnormalinvert invert="true" />
					<setmkbound mk="0" />
					<drawbox>
                        <boxfill>top | bottom</boxfill>
                        <point x="#tank_min_x" y="#-dom_padding" z="#tank_min_z+Dp/2" />
                        <size x="#tank_max_x-tank_min_x" y="#2*dom_padding" z="#tank_max_z-tank_min_z-Dp" />
                    </drawbox>
                    <setnormalinvert invert="false" />
                    <setmkbound mk="1" />
                    <drawcylinder radius="#cyl_radius+Dp/2">
                        <point x="#cyl_cen_x" y="#-dom_padding" z="#cyl_cen_z" />
                        <point x="#cyl_cen_x" y="#dom_padding" z="#cyl_cen_z" />
                    </drawcylinder>
                    <setmkbound mk="2" />
                    <drawbox>
                        <boxfill>top | bottom | right</boxfill>
                        <point x="#struc_min_x" y="#-dom_padding" z="#struc_min_z-Dp/2" />
                        <size x="#struc_max_x-struc_min_x+Dp/2" y="#2*dom_padding" z="#struc_max_z-struc_min_z+Dp" />
                    </drawbox>
                    <shapeout file="hdp" />
                    <resetdraw />
                </list>
                <mainlist>
                    <newvarcte tank_min_x="#fluid_min_x-(inout_layers+4)*Dp" tank_min_z="#fluid_min_z" />
                    <newvarcte tank_max_x="#fluid_max_x+(inout_layers+4)*Dp" tank_max_z="#fluid_max_z" />
                    <runlist name="GeometryForNormals" />
                    <setdrawmode mode="full" />
                    <setmkfluid mk="0" />
                    <drawbox>
                        <boxfill>solid</boxfill>
                        <point x="#fluid_min_x" y="#-dom_padding" z="#fluid_min_z" />
                        <size x="#fluid_max_x-fluid_min_x" y="#2*dom_padding" z="#fluid_max_z-fluid_min_z" />
                    </drawbox>
                    <setmkfluid mk="1" />
                    <drawbox>
                        <boxfill>left</boxfill>
                        <point x="#fluid_min_x" y="#-dom_padding" z="#fluid_min_z" />
                        <size x="#fluid_max_x-fluid_min_x" y="#2*dom_padding" z="#fluid_max_z-fluid_min_z" />
                    </drawbox>
                    <setmkfluid mk="2" />
                    <drawbox>
                        <boxfill>right</boxfill>
                        <point x="#fluid_min_x" y="#-dom_padding" z="#fluid_min_z" />
                        <size x="#fluid_max_x-fluid_min_x" y="#2*dom_padding" z="#fluid_max_z-fluid_min_z" />
                    </drawbox>
					<setmkbound mk="0" />
					<drawbox>
                        <boxfill>top | bottom</boxfill>
                        <point x="#tank_min_x" y="#-dom_padding" z="#tank_min_z" />
                        <size x="#tank_max_x-tank_min_x" y="#2*dom_padding" z="#tank_max_z-tank_min_z" />
                        <layers vdp="0,1,2,3,4" />
                    </drawbox>
                    <setfrdrawmode auto="#cyl_freedraw" />
                    <setmkbound mk="1" />
                    <drawcylinder radius="#cyl_radius">
                        <point x="#cyl_cen_x" y="#-dom_padding" z="#cyl_cen_z" />
                        <point x="#cyl_cen_x" y="#dom_padding" z="#cyl_cen_z" />
                    </drawcylinder>
                    <setfrdrawmode auto="false" />
                    <setmkbound mk="2" />
                    <drawbox>
                        <boxfill>solid</boxfill>
                        <point x="#struc_min_x" y="#-dom_padding" z="#struc_min_z" />
                        <size x="#struc_max_x-struc_min_x" y="#2*dom_padding" z="#struc_max_z-struc_min_z" />
                    </drawbox>
                </mainlist>
            </commands>
        </geometry>
        <normals active="true">
            <norgeometry comment="Define initial configuration of normals before applying other configurations">
                <geometryfile file="[CaseName]_hdp_Actual.vtk" comment="File with boundary geometry (VTK format)" />
                <distanceh v="2" comment="Maximum orthogonal distance (H*value) to compute normals data (default=2)" />
                <maxsizeh v="0" comment="Maximum allowed normal size (H*value) (disabled=0, default=0)" />
                <svshapes v="true" comment="Saves VTK with geometry in triangles and quads with its normals for debug (default=false)" />
            </norgeometry>
        </normals>
        <motion>
            <objreal ref="2">
                <begin mov="1" start="0" />
                <mvnull id="1" />
            </objreal>
        </motion>
    </casedef>
    <execution>
		<special>
            <inout>
                <memoryresize size0="2" size="4" comment="Initial memory resize (size0) and the following memory resizes according to initial inlet/outlet particles (default=2 and 4)"/>
                <determlimit value="1e-3" comment="Use 1e-3 for first_order or 1e+3 for zeroth_order (default=1e+3)" />
                <extrapolatemode value="3" comment="Calculation mode for rhop and velocity extrapolation from ghost nodes 1:fast-single, 2:single, 3:double (default=1)" />
                <inoutzone>
                    <refilling value="0" comment="Refilling mode. 0:Simple full, 1:Simple below zsurf, 2:Advanced for reverse flows (very slow) (default=1)" />
                    <inputtreatment value="2" comment="Treatment of fluid entering the zone. 0:No changes, 1:Convert fluid (necessary for outlet), 2:Remove fluid (recommended for inlet)" />
                    <layers value="#inout_layers" comment="Number of inlet/outlet particle layers" />
                    <zone2d comment="Input zone for 2-D simulations">
                        <particles mkfluid="1" direction="right" />
                    </zone2d>
                    <imposevelocity mode="1" comment="Imposed velocity 0:fixed value, 1:variable value, 2:Extrapolated value (default=0)">
                        <velocitytimes3 comment="Parabolic inlet velocity in time">
                            <timevalue time="0.0" v="0" v2="0.000000000000000" v3="0.0" z="0.0" z2="0.205" z3="0.41" />
                            <timevalue time="0.2" v="0" v2="0.036707612778635" v3="0.0" z="0.0" z2="0.205" z3="0.41" />
                            <timevalue time="0.4" v="0" v2="0.143237254218789" v3="0.0" z="0.0" z2="0.205" z3="0.41" />
                            <timevalue time="0.6" v="0" v2="0.309161060780645" v3="0.0" z="0.0" z2="0.205" z3="0.41" />
                            <timevalue time="0.8" v="0" v2="0.518237254218789" v3="0.0" z="0.0" z2="0.205" z3="0.41" />
                            <timevalue time="1.0" v="0" v2="0.750000000000000" v3="0.0" z="0.0" z2="0.205" z3="0.41" />
                            <timevalue time="1.2" v="0" v2="0.981762745781211" v3="0.0" z="0.0" z2="0.205" z3="0.41" />
                            <timevalue time="1.4" v="0" v2="1.190838939219355" v3="0.0" z="0.0" z2="0.205" z3="0.41" />
                            <timevalue time="1.6" v="0" v2="1.356762745781211" v3="0.0" z="0.0" z2="0.205" z3="0.41" />
                            <timevalue time="1.8" v="0" v2="1.463292387221365" v3="0.0" z="0.0" z2="0.205" z3="0.41" />
                            <timevalue time="2.0" v="0" v2="1.500000000000000" v3="0.0" z="0.0" z2="0.205" z3="0.41" />
                        </velocitytimes3>
                    </imposevelocity>
                    <imposerhop mode="2" comment="Outlet rhop 0:Imposed fixed value, 1:Hydrostatic, 2:Extrapolated from ghost nodes (default=0)"/>
                    <imposezsurf mode="0" comment="Inlet Z-surface 0:Imposed fixed value, 1:Imposed variable value, 2:Calculated from fluid domain (default=0)">
                        <zsurf value="#fluid_max_z" comment="Characteristic inlet Z-surface (used for Hydrostatic option)" units_comment="m" />
                        <remove value="false" comment="Removes particles above the Zsurf limit (default=false)" />
                    </imposezsurf>
                </inoutzone>
                <inoutzone>
                    <refilling value="2" comment="Refilling mode. 0:Simple full, 1:Simple below zsurf, 2:Advanced for reverse flows (very slow) (default=1)" />
                    <inputtreatment value="1" comment="Treatment of fluid entering the zone. 0:No changes, 1:Convert fluid (necessary for outlet), 2:Remove fluid (recommended for inlet)" />
                    <layers value="#inout_layers" comment="Number of inlet/outlet particle layers" />
                    <zone2d comment="Input zone for 2-D simulations">
                        <particles mkfluid="2" direction="left" />
                    </zone2d>
                    <imposevelocity mode="2" comment="Imposed velocity 0:fixed value, 1:variable value, 2:Extrapolated value (default=0)" />
                    <imposerhop mode="0" comment="Outlet rhop 0:Imposed fixed value, 1:Hydrostatic, 2:Extrapolated from ghost nodes (default=0)"/>
                    <imposezsurf mode="0" comment="Inlet Z-surface 0:Imposed fixed value, 1:Imposed variable value, 2:Calculated from fluid domain (default=0)">
                        <zsurf value="#fluid_max_z" comment="Characteristic inlet Z-surface (used for Hydrostatic option)" units_comment="m" />
                        <remove value="false" comment="Removes particles above the Zsurf limit (default=false)" />
                    </imposezsurf>
                </inoutzone>
            </inout>
            <flexstrucs>
                <flexstrucbody mkbound="2" mkclamp="1">
                    <density value="10000.0" comment="Mass density" units_comment="kg/m^3" />
                    <youngmod value="1.4e6" comment="Young Modulus" units_comment="Pa" />
                    <poissratio value="0.4" comment="Poisson ratio" />
                    <constitmodel value="1" comment="Constitutive model 1:Plane Strain (2D), 2:Plane Stress (2D), 3:St. Venant Kirchhoff (3D)" />
                    <hgfactor value="0.1" comment="Hourglass correction factor: keep as low as possible (default=0)" />
                </flexstrucbody>
            </flexstrucs>
		</special>
        <parameters>
            <parameter key="Boundary" value="#eval(mdbc,2,1)" comment="Boundary method 1:DBC, 2:mDBC (default=1)" />
            <parameter key="SlipMode" value="2" comment="Slip mode for mDBC 1:DBC vel=0, 2:No-slip, 3:Free slip (default=1)" />
            <parameter key="NoPenetration" value="0" comment="Prevent particle penetration in mDBC 0:Off, 1:On (default=0)" />
            <parameter key="StepAlgorithm" value="2" comment="Step Algorithm 1:Verlet, 2:Symplectic (default=1)" />
            <parameter key="Kernel" value="2" comment="Interaction Kernel 1:Cubic Spline, 2:Wendland (default=2)" />
            <parameter key="ViscoTreatment" value="2" comment="Viscosity formulation 1:Artificial, 2:Laminar+SPS (default=1)" />
            <parameter key="Visco" value="0.001" comment="Viscosity value" /> % Note alpha can depend on the resolution. A value of 0.01 is recommended for near irrotational flows.
            <parameter key="ViscoBoundFactor" value="1" comment="Multiply viscosity value with boundary (default=1)" />
			<parameter key="DensityDT" value="1" comment="Density Diffusion Term 0:None, 1:Molteni, 2:Fourtakas, 3:Fourtakas(full) (default=0)" />
			<parameter key="DensityDTvalue" value="0.1" comment="DDT value (default=0.1)" />
            <parameter key="Shifting" value="3" comment="Shifting mode 0:None, 1:Ignore bound, 2:Ignore fixed, 3:Full, 4:Full advanced (default=0)" />
            <parameter key="ShiftCoef" value="-20" comment="Coefficient for standard shifting computation (default=-2)" />
            <parameter key="ShiftTFS" value="0.0" comment="Threshold to detect free surface. Typically 1.5 for 2D and 2.75 for 3D (default=0)" />
            <parameter key="CoefDtMin" value="0.001" comment="Coefficient to calculate minimum time step dtmin=coefdtmin*h/speedsound (default=0.05)" />
			<parameter key="DtAllParticles" value="1" comment="Velocity of particles used to calculate DT. 1:All, 0:Only fluid/floating (default=0)" />
            <parameter key="TimeMax" value="15.0" comment="Time of simulation" units_comment="seconds" />
            <parameter key="TimeOut" value="0.05" comment="Time out data" units_comment="seconds" />
            <parameter key="MinFluidStop" value="0.5" comment="Minimum proportion of initial fluid particles to continue the simulation 0:Never stops, 0.1:Stops when fluid drops to 10%. (default=0)" />
            <simulationdomain comment="Defines domain of simulation (default=Uses minimum and maximum position of the generated particles)">
                <posmin x="default - 5%" y="default" z="default - 5%" comment="e.g.: x=0.5, y=default-1, z=default-10%" />
                <posmax x="default + 5%" y="default" z="default + 5%" />
            </simulationdomain>
        </parameters>
    </execution>
</case>
