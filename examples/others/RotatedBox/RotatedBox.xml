                        <size x="0.8" y="0.5" z="0.15" />
                    </drawbox>
                    <!-- Floor -->
                    <setmkbound mk="1" />
                    <drawbox>
                        <boxfill>bottom</boxfill>
                        <point x="0" y="0.0" z="0" />
                        <size x="0.8" y="0.5" z="0.15" />
                    </drawbox>
                </mainlist>
            </commands>
