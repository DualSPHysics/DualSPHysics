<?xml version="1.0" encoding="UTF-8" ?>
<case>
    <casedef>
        <constantsdef>
            <lattice bound="1" fluid="1" />			
            <gravity x="0" y="0" z="-9.81" comment="Gravitational acceleration" units_comment="m/s^2" />
            <rhop0 value="997" comment="Reference density of the fluid" units_comment="kg/m^3" />
            <hswl value="0" auto="true" comment="Maximum still water level to calculate speedofsound using coefsound" units_comment="metres (m)" />
            <gamma value="7" comment="Polytropic constant for water used in the state equation" />
            <speedsystem value="10" auto="false" comment="Maximum system speed (by default the dam-break propagation is used)" />
            <coefsound value="10" comment="Coefficient to multiply speedsystem" />
            <speedsound value="0" auto="true" comment="Speed of sound to use in the simulation (by default speedofsound=coefsound*speedsystem)" />
            <coefh value="1.0" comment="Coefficient to calculate the smoothing length (h=coefh*sqrt(3*dp^2) in 3D)" />		
            <cflnumber value="0.1" comment="Coefficient to multiply dt" />	
        </constantsdef>	
        <mkconfig boundcount="240" fluidcount="10" />
        <geometry>
            <definition dp="0.001" units_comment="metres (m)">
                <pointmin x="-0.008" y="-0.008" z="-0.018" />
                <pointmax x="0.808" y="0.208" z="0.604" />
            </definition>
            <commands>
                <mainlist>
                    <setdrawmode mode="full" />
                    <setmkfluid mk="0" />
                    <drawbox>
                        <boxfill>solid</boxfill>
                        <point x="0.0" y="0.0" z="0.0" />
                        <size x="0.2" y="0.2" z="0.4" />
                    </drawbox>
					<setmkbound mk="0" />
					<drawbox>
                        <boxfill>solid</boxfill>
                        <point x="-0.004" y="-0.004" z="-0.014" />
                        <size x="0.004" y="0.208" z="0.614" />
                    </drawbox>
					<drawbox>
                        <boxfill>solid</boxfill>
                        <point x="0.8" y="-0.004" z="-0.014" />
                        <size x="0.004" y="0.208" z="0.614" />
                    </drawbox>
					<setmkbound mk="1" />
					<drawbox>
                        <boxfill>solid</boxfill>
                        <point x="-0.004" y="-0.004" z="-0.014" />
                        <size x="0.808" y="0.004" z="0.614" />
                    </drawbox>
					<drawbox>
                        <boxfill>solid</boxfill>
                        <point x="-0.004" y="0.2" z="-0.014" />
                        <size x="0.808" y="0.004" z="0.614" />
                    </drawbox>
					<setmkbound mk="2" />
					<drawbox>
                        <boxfill>solid</boxfill>
                        <point x="0.2" y="0.001" z="0.0" />
                        <size x="0.004" y="0.198" z="0.5" />
                    </drawbox>
					<setmkbound mk="3" />
					<drawbox>
                        <boxfill>solid</boxfill>
                        <point x="-0.004" y="-0.004" z="-0.014" />
                        <size x="0.808" y="0.208" z="0.014" />
                    </drawbox>
					<setmkbound mk="4" />
					<drawbox>
                        <boxfill>solid</boxfill>
                    	<point x="0.596" y="0.001" z="-0.01" />
                    	<size x="0.004" y="0.198" z="0.1" />
                    </drawbox>
                </mainlist>
            </commands>
        </geometry>
        <motion>
            <objreal ref="2">
                <begin mov="1" start="0.0" finish="0.12" />
                <mvfile id="1" duration="0.12">
                    <file name="Gate.dat" fields="2" fieldtime="0" fieldz="1" />
                </mvfile>
            </objreal>
        </motion>
    </casedef>
    <execution>
		<special>
			<structures>
				<structure mkbound="4">
					<Density value="1161.54" comment="Structure material density" units_comment="kg/m^3" />
					<YoungMod value="3.5e6" comment="Young's Modulus" units_comment="Pa" />
					<PoissonRatio value="0.49" comment="Poisson ratio" />
					<ConstModel value="StVenantKirchhoff" comment="Constitutive model (PlaneStrain, PlaneStress, StVenantKirchhoff)" />
					<HGFactor value="0.1" comment="Hourglass correction factor: keep as low as possible (default=0)" />
				</structure>
			</structures>
		</special>
        <parameters>
	        <parameter key="DomainFixedZmax" value="1.2" comment="The domain is fixed in the specified limit (default=not applied)" units_comment="metres (m)" />
            <parameter key="StepAlgorithm" value="1" comment="Step Algorithm 1:Verlet, 2:Symplectic (default=1)" />
			<parameter key="VerletSteps" value="40" comment="Verlet only: Number of steps to apply Euler timestepping (default=40)" />
            <parameter key="Kernel" value="2" comment="Interaction Kernel 1:Cubic Spline, 2:Wendland (default=2)" />
            <parameter key="ViscoTreatment" value="2" comment="Viscosity formulation 1:Artificial, 2:Laminar+SPS (default=1)" />
            <parameter key="Visco" value="8.92678034e-7" comment="Viscosity value" /> % Note alpha can depend on the resolution. A value of 0.01 is recommended for near irrotational flows.
            <parameter key="DensityDT" value="2" comment="Density Diffusion Term 0:None, 1:Molteni, 2:Fourtakas, 3:Fourtakas(full) (default=0)" />
			<parameter key="DensityDTvalue" value="0.1" comment="DDT value (default=0.1)" />
            <parameter key="CoefDtMin" value="0.001" comment="Coefficient to calculate minimum time step dtmin=coefdtmin*h/speedsound (default=0.05)" />
			<parameter key="DtAllParticles" value="1" comment="Velocity of particles used to calculate DT. 1:All, 0:Only fluid/floating (default=0)" />
            <parameter key="TimeMax" value="0.5" comment="Time of simulation" units_comment="seconds" />
            <parameter key="TimeOut" value="0.01" comment="Time out data" units_comment="seconds" />
        </parameters>
    </execution>
</case>
